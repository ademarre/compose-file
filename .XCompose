# circumflexes
<Multi_key> <^> <a> : "â" # Latin a with circumflex
<Multi_key> <^> <A> : "Â" # Latin A with circumflex
<Multi_key> <^> <e> : "ê" # Latin e with circumflex
<Multi_key> <^> <E> : "Ê" # Latin E with circumflex
<Multi_key> <^> <i> : "î" # Latin i with circumflex
<Multi_key> <^> <I> : "Î" # Latin I with circumflex
